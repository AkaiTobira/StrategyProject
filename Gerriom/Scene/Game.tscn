[sub_resource type="GDScript" id=5]
script/source = "extends Control

var _unit_id   = 0

func _on_TextureButton2_mouse_entered():
	get_node(\"../../../UnitPreviev\").fill_preview( _unit_id )

func _input(event):
	if not Utils.mouse_unit_move_marker.is_active: return
	if event is InputEventMouseMotion:
		Utils.mouse_unit_move_marker.position = get_viewport().get_mouse_position()#get_global_mouse_position()
	
	if event is InputEventMouseButton:
		if not event.is_pressed() and event.button_index == BUTTON_LEFT:
			Utils.mouse_unit_move_marker.position  = Vector2(-1000, -1000)
			Utils.mouse_unit_move_marker.is_active = false
			Utils.mouse_unit_move_marker.pressed_button = null

func _on_TextureButton2_button_down():
	Utils.mouse_unit_move_marker.is_active      = true
	Utils.mouse_unit_move_marker.active_unit_id = _unit_id
	Utils.mouse_unit_move_marker.pressed_button = self
	pass # Replace with function body.
"

